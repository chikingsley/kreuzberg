{
	"id": "ocr_paddle_element_levels",
	"category": "ocr",
	"description": "Tests PaddleOCR with word-level element extraction",
	"document": {
		"path": "images/test_hello_world.png",
		"media_type": "image/png",
		"requires_external_tool": ["paddle-ocr", "onnxruntime"]
	},
	"extraction": {
		"config": {
			"ocr": {
				"backend": "paddle-ocr",
				"language": "en",
				"element_config": {
					"include_elements": true,
					"min_level": "word"
				}
			},
			"force_ocr": true
		}
	},
	"assertions": {
		"expected_mime": "image/png",
		"min_content_length": 5,
		"ocr_elements": {
			"has_elements": true,
			"elements_have_geometry": true,
			"min_count": 1
		}
	},
	"skip": {
		"requires_feature": ["paddle-ocr"],
		"notes": "Requires PaddleOCR with ONNX Runtime"
	}
}
