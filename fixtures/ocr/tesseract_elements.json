{
	"id": "ocr_tesseract_elements",
	"category": "ocr",
	"description": "Tests Tesseract OCR with element-level structured output including geometry",
	"document": {
		"path": "images/test_hello_world.png",
		"media_type": "image/png",
		"requires_external_tool": ["tesseract"]
	},
	"extraction": {
		"config": {
			"ocr": {
				"backend": "tesseract",
				"language": "eng",
				"element_config": {
					"include_elements": true
				}
			},
			"force_ocr": true
		}
	},
	"assertions": {
		"expected_mime": "image/png",
		"min_content_length": 5,
		"ocr_elements": {
			"has_elements": true,
			"elements_have_geometry": true,
			"elements_have_confidence": true
		}
	},
	"skip": {
		"requires_feature": ["tesseract"],
		"notes": "Requires Tesseract OCR backend"
	}
}
